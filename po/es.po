msgid "<b>Add new user to SAMBA server:</b>"
msgstr "<b>Añadir nuevo usuario al servidor SAMBA:</b>"

#: ../src/property-page.c:117
msgid "    sudo smbpasswd -a <i>your_username</i>"
msgstr "    sudo smbpasswd -a <i>su_usuario</i>"

