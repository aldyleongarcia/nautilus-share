msgid "<b>Add new user to sambashare group:</b>"
msgstr "<b>Añadir nuevo usuario al grupo sambashare:</b>"

#: ../src/property-page.c:119
msgid "    sudo usermod -a -G sambashare  <i>your_username</i> "
msgstr "    sudo usermod -a -G sambashare  <i>su_usuario</i> "

